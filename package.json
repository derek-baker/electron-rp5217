{
  "name": "rp5217",
  "version": "1.0.0",
  "license": "MIT",
  "description": "app to generate pdfs with 417 barcode",
  "author": "Systems Development Group <dbaker@sdgnys.com>",
  "main": "main.js",
  "scripts": {
    "postinstall": "install-app-deps",
    "start": "SET \"NODE_ENV=dev\" && electron . \"..\\data.sdg\"",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "build": "webpack",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "build": {
    "appId": "com.electron.rp5217",
    "artifactName": "RP5217 Setup.exe",
    "directories": {
      "output": "electron_dist"
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "electron_build_resources/icon.ico"
    },
    "nsis": {
      "allowElevation": true,
      "installerIcon": "electron_build_resources/icon.ico",
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "RP5217"
    },
    "fileAssociations": {
      "ext": "sdg",
      "name": "SDGFILE"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "css-loader": "^1.0.1",
    "electron": "latest",
    "electron-builder": "^20.28.4",
    "electron-builder-squirrel-windows": "^20.29.0",
    "ts-loader": "^5.3.0",
    "typescript": "^3.1.6",
    "vue": "^2.5.17",
    "vue-loader": "^15.4.2",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2"
  }
}
